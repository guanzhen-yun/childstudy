<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.inke.childstudy">
    <!--允许联网 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <!--获取GSM（2g）、WCDMA（联通3g）等网络状态的信息  -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <!--获取wifi网络状态的信息 -->
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <!--保持CPU 运转，屏幕和键盘灯有可能是关闭的,用于文件上传和下载 -->
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <!--获取sd卡写的权限，用于文件上传和下载-->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <!--允许读取手机状态 用于创建BmobInstallation-->
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
<!--    &lt;!&ndash; 外部存储读权限，调试工具从SD卡加载本地补丁需要。&ndash;&gt;-->
<!--    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />-->

    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera2" />

    <application
        android:name=".SophixStubApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:supportsRtl="true"
        android:theme="@style/Theme.ChildStudy">
        <activity
            android:name=".SplashActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name=".regist.RegistActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".home.HomeActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".login.LoginActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".set.SetActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".userinfo.UserInfoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".userinfo.BigHeadActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".studycolor.StudyColorActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".word.StudyWordActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".studyobject.StudyObjectActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".studyobject.BigPicActivity"
            android:screenOrientation="portrait" />

        <provider
            android:name="cn.bmob.v3.util.BmobContentProvider"
            android:authorities="com.inke.childstudy.BmobContentProvider"></provider>

        <provider
            android:name="androidx.core.content.FileProvider"
            android:authorities="com.inke.childstudy.fileprovider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/file_paths" />
            <meta-data
                android:name="com.taobao.android.hotfix.IDSECRET"
                android:value="333518348" />
            <meta-data
                android:name="com.taobao.android.hotfix.APPSECRET"
                android:value="9f4ffbfa7f9d42ab9da3c9ea59c0465c" />
            <meta-data
                android:name="com.taobao.android.hotfix.RSASECRET"
                android:value="" />
        </provider>

        <meta-data
            android:name="channel"
            android:value="${APP_CHANNEL_VALUE}" />
    </application>

</manifest>